{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "esnext",
    "outDir": "dist",
    "target": "es2015",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "allowJs": true,
    "resolveJsonModule":true
  },
}
